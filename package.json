{
	"name": "ps-ecommerce",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"__dev": "npm run dev:graphql-router & nx run-many --target=serve --all=true --parallel=true",
		"dev:frontend": "nx run-many --target=serve --projects=catalog-frontend,customer-frontend,checkout-frontend --parallel=true",
		"dev:backend": "npm run dev:graphql-router & nx run-many --target=serve --projects=catalog-service,orders-service,wishlist-service,checkout-service --parallel=true --maxParallel=5",
		"dev:graphql-router": "./apps/apollo-router/router --hot-reload --log info --config ./apps/apollo-router/config.yaml --supergraph ./apps/apollo-router/schemas/supergraph-schema.graphql",
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"graph": "nx graph --watch",
		"codegen": "graphql-codegen --config codegen.ts"
	},
	"private": true,
